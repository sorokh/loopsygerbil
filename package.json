{
  "name": "loopsy",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "lint": "node node_modules/eslint/bin/eslint.js --cache -o ./build/eslint.html -f html .",
    "start": "node .",
    "dev": "nodemon server/server.js",
    "docker:dev": "NODE_ENV=docker nodemon server/server.js",
    "docker:test": "NODE_ENV=docker; node node_modules/mocha/bin/mocha",
    "docker:test:report": "NODE_ENV=docker; mocha --reporter mochawesome --reporter-options reportDir=./build/mochawesome",
    "docker:test:watch": "NODE_ENV=docker; npm run test -- -w",
    "docker:test:coverage": "NODE_ENV=docker; node node_modules/istanbul/lib/cli.js cover --dir ./build/coverage node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 ",
    "posttest": "npm run lint && nsp check",
    "test": "node node_modules/mocha/bin/mocha",
    "test:report": "mocha --reporter mochawesome --reporter-options reportDir=./build/mochawesome",
    "test:watch": "npm run test -- -w",
    "db-migrate": "node node_modules/db-migrate/bin/db-migrate",
    "coverage": "node node_modules/istanbul/lib/cli.js cover --dir ./build/coverage node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 ",
    "outdated": "npm outdated --depth=0",
    "angular": "node node_modules/loopback-sdk-angular-cli/bin/lb-ng.js -m gerbilServices -u http://192.168.99.100:32772/api server/server.js client/js/gerbilService.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^3.5.0",
    "loopback-jsonapi-model-serializer": "1.0.1",
    "loopback": "^2.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-access-groups": "^1.0.3",
    "loopback-context": "^3.1.0",
    "loopback-component-jsonapi": "^2.2.1",
    "loopback-component-passport": "^2.3.0",
    "loopback-component-storage": "^1.5.0",
    "loopback-connector-postgresql": "^2.7.0",
    "loopback-datasource-juggler": "^2.39.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.2",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "mochawesome": "^2.0.4",
    "istanbul": "^0.4.5",
    "uuid": "3.0.x",
    "chai": "^4.0.2",
    "chai-datetime": "^1.4.1",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.18.0",
    "eslint-config-loopback": "^8.0.0",
    "loopback-component-explorer": "^2.7.0",
    "nsp": "^2.1.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "",
    "loopback-supertest": "^0.1.11",
    "loopback-testing": "^1.4.0",
    "loopback-sdk-angular-cli": "2.3.0",
    "nodemon": "^1.11.0"
  },
